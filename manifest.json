{
    "name": "Dark Theme for Instagram™",
    "version": "1.1.18",
    "description": "Dark Theme for Instagram™",
    "icons": {
        "128": "img/icon.png"
    },
    "manifest_version": 3,
    "content_scripts": [
        {
            "js": [
                "js/app.js"
            ],
            "css": [
                "css/contentstyle.css"
            ],
            "matches": [
                "*://*.instagram.com/*"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "css/style.css",
                "img/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "permissions": [
        "storage"
    ],
    "content_security_policy": {},
    "host_permissions": [
        "*://fonts.googleapis.com/"
    ]
}