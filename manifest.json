{
    "name": "Instagram Dark Theme",
    "version": "1.1.15",
    "description": "Dark Theme for Instagram",
    "icons": {
        "128": "img/icon.png"
    },
    "manifest_version": 2,
    "content_scripts": [
        {
            "js": [
                "js/app.js"
            ],
            "css": [
                "css/contentstyle.css"
            ],
            "matches": [
                "*://*.instagram.com/*"
            ]
        }
    ],
    "web_accessible_resources": [
        "css/style.css",
        "img/*"
    ],
    "permissions": [
        "storage",
        "*://fonts.googleapis.com/"
    ]
}